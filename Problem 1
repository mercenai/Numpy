import numpy as np
X=np.array([[0.62263025, 0.65083672, 0.22364161, 0.88096807, 0.17578497],[0.55783694, 0.69420414, 0.05998999, 0.21365965, 0.29602037],[0.21378004, 0.31575981, 0.74983932, 0.29194748, 0.77617749],[0.1200921 , 0.31854629, 0.56997779, 0.05459041, 0.79177759],[0.76849802, 0.55871737, 0.39768252, 0.91592833, 0.04950052]])
# X was created using np.random.random
m=X.mean()
s=X.std()
Z=(X-m)/s
print(Z)
